{
  "name": "insight-feeder-backend",
  "version": "1.0.0",
  "description": "A comprehensive news aggregation and AI enhancement system built with Node.js, Supabase, and Gemini AI",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "worker": "node scripts/worker.js",
    "worker:dev": "nodemon scripts/worker.js",
    "seed": "node scripts/seed-data.js",
    "seed:rss": "node scripts/seed-rss-feeds.js",
    "ai:test": "node scripts/test-ai.js",
    "crawl": "node scripts/run-crawl.js",
    "audit:ml": "node scripts/audit-multilang.js",
    "lang:backfill": "node scripts/backfill-language-strong.js",
    "reset:data": "node scripts/reset-data.js",
    "clear:news": "node scripts/clear-news.js",
    "clear:news:verify": "node scripts/verify-news-clear.js",
    "cluster:enrich": "node scripts/run-cluster-enricher.js",
    "logs:audit": "node scripts/audit-llm-logs.js",
    "logs:clean": "LLM_LOG_DIR=llm-logs node scripts/cleanup-llm-logs.js --purge && LLM_LOG_DIR=rss-logs node scripts/cleanup-llm-logs.js --purge && find rss-logs -type f ! -name '*.json' -delete 2>/dev/null || true",
    "media:test": "node scripts/test-media-extract.js",
    "media:backfill": "MEDIA_ENABLED=true node scripts/backfill-media.js",
    "media:audit": "node scripts/run-audit-media.js",
    "storage:verify": "node scripts/verify-storage.js",
    "source:policy": "node scripts/source-policy-set.js",
    "acl:smoke": "node scripts/acl-smoke.js",
    "db:rls": "node scripts/check-rls.js",
    "test:fulltext": "node scripts/test-fulltext.js",
    "migrate": "node scripts/run-migrations.js",
    "pretranslate": "node scripts/run-pretranslation.js",
    "test": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --forceExit",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --watch",
    "lint": "eslint \"src/**/*.js\" \"scripts/**/*.js\" --max-warnings=0"
  },
  "keywords": [
    "news",
    "rss",
    "ai",
    "supabase",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@mozilla/readability": "^0.4.4",
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsdom": "^24.0.0",
    "node-cron": "^3.0.3",
    "probe-image-size": "^7.2.3",
    "rss-parser": "^3.13.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "setupFiles": [
      "<rootDir>/tests/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {}
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
