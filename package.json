{
  "name": "insight-feeder-backend",
  "version": "1.0.0",
  "description": "A comprehensive news aggregation and AI enhancement system built with Node.js, Supabase, and Gemini AI",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "seed": "node scripts/seed-data.js",
    "test": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --forceExit",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "keywords": [
    "news",
    "rss",
    "ai",
    "supabase",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "cross-env": "^7.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "setupFiles": [
      "<rootDir>/tests/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {}
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
